{
  "name": "UI Builder for vue and react",
  "version": "0.0.1-alpha",
  "description": "A app where you will be able to build your components visually.",
  "repository": {
    "type": "git",
    "url": "git://github.com/baderxx/uibuilder"
  },
  "author": "baderxx",
  "license": "MIT",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "husky install",
    "prepare": "husky install",
    "lint": "eslint --ext \".ts,.js,.vue\" --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext \".ts,.js,.vue\" --ignore-path .gitignore ."
  },
  "devDependencies": {
    "@intlify/nuxt3": "^0.2.2",
    "@nuxt/types": "^2.15.8",
    "@nuxtjs/eslint-config-typescript": "^10.0.0",
    "@nuxtjs/eslint-module": "^3.1.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^3.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "lint-staged": "^12.4.1",
    "nuxt": "3.0.0-rc.3",
    "prettier": "^2.6.2",
    "tailwindcss": "^3.0.24"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.1.9",
    "pinia": "^2.0.14"
  },
  "lint-staged": {
    "**/*.{js,ts,vue,html,css,scss}": [
      "yarn lintfix"
    ]
  }
}
